{
  "name": "e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/e2e/src",
  "tags": [],
  "implicitDependencies": ["skautoteka-frontend"],
  "// targets": "to see all targets run: nx show project e2e --web",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/e2e/cypress.config.ts",
        "devServerTarget": "skautoteka-frontend:serve",
        "baseUrl": "http://localhost:4200"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "skautoteka-frontend:serve:ci",
          "headless": true,
          "browser": "chrome"
        }
      }
    }
  }
}
